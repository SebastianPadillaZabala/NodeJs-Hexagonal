{
    "compilerOptions": {
      "target": "ES2021",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "outDir": "dist",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  